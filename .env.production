# Nuuk Bus Live Map - Production Configuration

# Realtime feed org_id (default: 968 for Nuuk)
VITE_ORG_ID=968

# Polling interval in milliseconds (default: 8000 = 8 seconds)
VITE_POLL_MS=8000

# Map tile configuration - CartoDB Voyager (clean style with road names)
VITE_TILE_URL=https://{s}.basemaps.cartocdn.com/rastertiles/voyager/{z}/{x}/{y}{r}.png
VITE_TILE_ATTRIBUTION=&copy; <a href="https://www.openstreetmap.org/copyright">OpenStreetMap</a> contributors &copy; <a href="https://carto.com/attributions">CARTO</a>

# Feature flags
VITE_ENABLE_STATIC_LAYERS=false
VITE_ENABLE_ROUTES_LAYER=true

# Production API base URL - Cloudflare Worker for realtime proxy
VITE_API_BASE_URL=https://nuuk-bus-realtime.by-ole-olsvig.workers.dev
